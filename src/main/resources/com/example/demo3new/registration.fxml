    <Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="538.0" prefWidth="330.0">
        <children>
            <Text layoutX="14.0" layoutY="17.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registration Page">
                <font>
                    <Font name="Ageo-ExtraBold" size="12.0" />
